{
  "name": "mobile-clean",
  "version": "1.5.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "start:clear": "rm -rf node_modules/.cache && expo start --clear",
    "android": "expo run:android",
    "android:fresh": "rm -rf node_modules/.cache && expo run:android",
    "ios": "expo run:ios",
    "ios:fresh": "rm -rf node_modules/.cache && expo run:ios",
    "ios:fix": "node scripts/fix-podfile.js && expo run:ios",
    "ios:rebuild": "node scripts/fix-podfile.js && cd ios && rm -rf build && npx expo prebuild --platform ios && node ../scripts/fix-podfile.js && pod install && cd .. && expo run:ios",
    "ios:clean": "cd ios && rm -rf build && pod install && cd .. && rm -rf node_modules/.cache && expo run:ios",
    "web": "expo start --web",
    "cache:clear": "rm -rf node_modules/.cache && watchman watch-del-all 2>/dev/null || true",
    "postinstall": "node scripts/fix-podfile.js"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-native-firebase/analytics": "^23.3.1",
    "@react-native-firebase/app": "^23.3.1",
    "@react-native-firebase/crashlytics": "^23.3.1",
    "@react-native-google-signin/google-signin": "^16.0.0",
    "@react-native-picker/picker": "2.11.1",
    "@react-navigation/bottom-tabs": "^7.4.7",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/native-stack": "^7.3.26",
    "@types/react-native-modal": "^3.1.1",
    "expo": "54.0.12",
    "expo-apple-authentication": "^8.0.7",
    "expo-build-properties": "^1.0.9",
    "expo-device": "~8.0.9",
    "expo-font": "~14.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-location": "~19.0.7",
    "expo-modules-core": "^3.0.20",
    "expo-notifications": "~0.32.12",
    "expo-status-bar": "~3.0.8",
    "i18next": "^25.5.2",
    "lucide-react-native": "^0.543.0",
    "react": "19.1.0",
    "react-i18next": "^15.7.3",
    "react-native": "0.81.4",
    "react-native-biometrics": "^3.0.1",
    "react-native-dotenv": "^3.4.11",
    "react-native-maps": "1.20.1",
    "react-native-modal": "^14.0.0-rc.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-vector-icons": "^10.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.1.10",
    "@types/react-native-vector-icons": "^6.4.18",
    "babel-preset-expo": "^54.0.1",
    "dotenv": "^17.2.3",
    "typescript": "~5.9.2"
  },
  "private": true
}
